timeout 30
gfxmenu /boot/Rainbow
default 0

title <<< Back to Main Menu" 
configfile /menu.lst

title Win 98
find --set-root /IMG/win98.ima
map /IMG/win98.ima (0xff) || map --mem /IMG/win98.ima (0xff)
map --hook
chainloader (0xff)

title XP Inst
find --set-root /IMG/xp_inst.gz
map /IMG/xp_inst.gz (0xff) || map --mem /IMG/xp_inst.gz (0xff)
map --hook
chainloader (0xff)

title Kon-Boot V2.1
map --mem /boot/konboot.img (fd0)
map --hook
chainloader (fd0)+1
map (hd1) (hd0)
map --hook
rootnoverify (fd0)

title Kon-Boot V2.1 ISO
find --set-root /boot/kon-bootCD.iso
